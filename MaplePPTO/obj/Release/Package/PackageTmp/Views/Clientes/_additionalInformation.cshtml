@model MaplePPTO.ViewModels.ClientAdditionalInfomartionViewModel 
<h2 class="m-0">Información Asociada</h2>

<div class="card card-primary card-outline card-outline-tabs">
    <div class="card-header p-0 border-bottom-0">
        <ul class="nav nav-tabs" id="custom-tabs-four-tab" role="tablist">
            <li class="nav-item">
                <a class="nav-link active" id="custom-tabs-four-messages-tab" data-toggle="pill" href="#custom-tabs-four-messages" role="tab" aria-controls="custom-tabs-four-messages" aria-selected="true">
                    <font style="vertical-align: inherit;">
                        <font style="vertical-align: inherit;">Contratos</font>
                    </font>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" id="custom-tabs-four-settings-tab" data-toggle="pill" href="#custom-tabs-four-settings" role="tab" aria-controls="custom-tabs-four-settings" aria-selected="false">
                    <font style="vertical-align: inherit;">
                        <font style="vertical-align: inherit;">
                            Especificaciones Contrato
                        </font>
                    </font>
                </a>
            </li>
        </ul>
    </div>
    <div class="card-body">
        <div class="tab-content" id="custom-tabs-four-tabContent">
            <div class="tab-pane fade active show" id="custom-tabs-four-messages" role="tabpanel" aria-labelledby="custom-tabs-four-messages-tab">
                <div>
                    <table class="table table-bordered table-hover table-sm" id="gridContrato">
                        <thead>
                            <tr>
                                <th>CONTRATO</th>
                                <th>FEC. INICIAL</th>
                                <th>FEC. FINAL</th>
                                <th>PROSPECTO</th>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model.ContratosList != null)
                            {
                                foreach (var item in Model.ContratosList)
                                {
                                    <tr>
                                        <td>
                                            @item.Contrato1
                                            <input type="hidden" value="@item.id" class="id_Contrat" />
                                        </td>
                                        <td>@item.FecIni</td>
                                        <td>@item.FecFin</td>
                                        <td>@item.Prospecto</td>
                                    </tr>
                                }
            }
            else
            {
                        <tr>
                            <td colspan="4" class="bg-danger text-white">
                               <span>NOT DATA FOUND!</span>
                            </td>

                        </tr>
            }


                        </tbody>
                    </table>
                </div>
            </div>
            <div class="tab-pane fade" id="custom-tabs-four-settings" role="tabpanel" aria-labelledby="custom-tabs-four-settings-tab">
                Loading....
            </div>
        </div>
    </div>
    <!-- /.card -->
</div>